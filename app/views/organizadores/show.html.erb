<h1><%= @organizador.nombreCompleto %></h1>
<p id="notice"><%= notice %></p>

<p>
  <b>Nombre(s):</b>
  <%= @organizador.nombre + " " + @organizador.seg_nombre %>
</p>
<p>
  <b>Apellido(s):</b>
  <%= @organizador.apellido + " " + @organizador.seg_apellido %>
</p>
<p>
  <b>Cédula:</b>
  <%= @organizador.cedula %>
</p>
<p>
  <b>Fecha de Nacimiento:</b>
  <%= @organizador.fecha_nac %>
</p>
<p>
  <b>Teléfono:</b>
  <%= @organizador.telefono %>
</p>
<p>
  <b>Correo:</b>
  <%= @organizador.correo %>
</p>
<p>
  <b>Dirección:</b>
  <%= @organizador.direccion %>
</p>
<p>
  <b>Institución:</b>
  <%= @organizador.institucion %>
</p>
<p>
  <b>Nivel:</b>
  <%= @organizador.nivel %>
</p>
<p>
  <b>Coordinación:</b>
  <%= @organizador.coordinacion %>
  <% if @organizador.coordinador == 1 %>
  <%=   "(coordinador)" %>
  <% end %>
</p>S

