<%= form_for(@participante) do |f| %>
  <% if @participante.errors.any? %>
    <div id="error_explanation" class="error_explanation">
      <h2><%= pluralize(@participante.errors.count, "error prohibió", "errores prohibieron") %> guardar el organizador:</h2>

      <ul>
      <% @participante.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <table class="formulario">
    <tr>
      <th width=55%></th>
    </tr>
    <tr>
      <td>
        <div class="field">
          <%= f.label      :cedula %>
          <%= f.text_field :cedula, :size => 20, :maxlength => 8 %>
        </div>
      </td>
    </tr>
  </table>
  
  <br />
  
  <div class="actions">
    <%= f.submit %>
  </div>
  
<% end %>
