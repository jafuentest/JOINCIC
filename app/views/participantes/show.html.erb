<h1><%= @participante.nombre + " " + @participante.apellido %></h1>
<p id="notice"><%= notice %></p>

<p>
  <b>Cédula:</b>
  <%= @participante.ci_par %>
</p>

<p>
  <b>Nombre(s):</b>
  <%= @participante.nombre + " " + @participante.seg_nombre%>
</p>

<p>
  <b>Apellido(s):</b>
  <%= @participante.apellido + " " + @participante.seg_apellido %>
</p>

<p>
  <b>Fecha de nacimiento:</b>
  <%= @participante.fecha_nac %>
</p>

<p>
  <b>Teléfono:</b>
  <%= @participante.telefono %>
</p>

<p>
  <b>Correo:</b>
  <%= @participante.correo %>
</p>

<p>
  <b>Dirección:</b>
  <%= @participante.direccion %>
</p>

<p>
  <b>Institución:</b>
  <%= @participante.institucion %>
</p>

<p>
  <b>Nivel:</b>
  <%= @participante.nivel %>
</p>

<p>
  <b>Comió:</b>
  <%= @participante.comida %>
</p>

<p>
  <b>Zona:</b>
  <%= Zona.find(@participante.id_zona).nombre %>
</p>

<%= link_to 'Modificar', edit_participante_path(@participante) %> |
<%= link_to 'Volver a la lista', participantes_path %>
