<h1>Modificar: <%= @participante.nombreCompleto %></h1>
<p id="notice"><%= notice %></p>
<div align="center">
  <% if @hash.nil? %>
    <%= render "form_0" %>
    <br/>
    <%= link_to "Mostrar", @participante %> |
    <%= link_to "Volver a la lista", participantes_path %>
  <% else %>
    <%= render "form_1" %>
    <br/>
    <%= link_to "Cancelar", :controller => 'participantes', :action => 'show', :id => params[:id], :hash => @hash %>
  <% end %>
</div>
