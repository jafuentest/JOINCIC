<h1>Listing mesas_de_trabajo</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Tema</th>
    <th>Dia</th>
    <th>Hora ini</th>
    <th>Hora fin</th>
    <th>Lugar</th>
    <th>Capacidad</th>
    <th>Id ponente</th>
    <th>Id pat</th>
    <th colspan="3">Opciones</th>
  </tr>

<% @mesas_de_trabajo.each do |mesa_de_trabajo| %>
  <tr>
    <td><%= mesa_de_trabajo.titulo %></td>
    <td><%= mesa_de_trabajo.tema %></td>
    <td><%= mesa_de_trabajo.dia %></td>
    <td><%= mesa_de_trabajo.hora_ini %></td>
    <td><%= mesa_de_trabajo.hora_fin %></td>
    <td><%= mesa_de_trabajo.lugar %></td>
    <td><%= mesa_de_trabajo.capacidad %></td>
    <td><%= mesa_de_trabajo.id_ponente %></td>
    <td><%= mesa_de_trabajo.id_pat %></td>
    <td><%= link_to image_tag("/show.png", :alt => "Mostrar"), mesa_de_trabajo %></td>
    <td><%= link_to image_tag("/edit.png", :alt => "Modificar"), edit_mesa_de_trabajo_path(mesa_de_trabajo) %></td>
    <td><%= link_to image_tag("/delete.png", :alt => "Eliminar"), mesa_de_trabajo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mesa de trabajo', new_mesa_de_trabajo_path %>
